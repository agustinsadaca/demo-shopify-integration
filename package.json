  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "test": "jest --maxWorkers=25%",
    "test-cli": "jest --coverage --coverageReporters='text-summary'",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:cov": "jest --coverage --maxWorkers=25%",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js -d ./ormconfig.ts",
    "migration:generate": "npm run build && npm run typeorm migration:generate --",
    "migration:create": "npm run build && npm run typeorm migration:create --",
    "migration:run": "npm run build && npm run typeorm migration:run",
    "migration:revert": "npm run build && npm run typeorm migration:revert"
  },
